{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/[[...catchall]].js"
        }
    ],
    "builds": [
        {
            "src": "next.config.mjs",
            "use": "@vercel/next"
        }
    ]
}